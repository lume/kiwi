{
	"name": "@lume/kiwi",
	"version": "0.3.2",
	"homepage": "https://github.com/lume/kiwi",
	"repository": "https://github.com/lume/kiwi",
	"type": "module",
	"main": "es/kiwi.js",
	"module": "es/kiwi.js",
	"types": "es/kiwi.d.ts",
	"description": "High speed Cassowary constraint solver in JavaScript.",
	"keywords": [
		"cassowary",
		"kiwi",
		"kiwi.js",
		"constraint solver",
		"autolayout"
	],
	"license": "BSD-3-Clause",
	"bugs": {
		"url": "https://github.com/lume/kiwi/issues"
	},
	"files": [
		"src",
		"lib",
		"es",
		"COPYING.txt"
	],
	"// Note, we pinned jsdoc to 3.6.1 to prevent an in-range breaking change, https://github.com/jsdoc/jsdoc/issues/1977": "",
	"devDependencies": {
		"benchmark": "^2.1.4",
		"cassowary": "0.0.2",
		"chai": "^4.2.0",
		"codecov": "^3.5.0",
		"istanbul": "^0.4.5",
		"jsdoc-to-markdown": "^4.0.1",
		"mocha": "^6.2.0",
		"prettier": "^2.3.0",
		"rimraf": "^3.0.2",
		"rollup": "1.17.0",
		"rollup-plugin-uglify": "6.0.2",
		"typescript": "^3.5.3"
	},
	"scripts": {
		"build": "tsc && tsc -t es6 --outDir tmp/es && rollup -c",
		"clean": "rimraf lib/ es/ tmp/",
		"test": "npm run lint && mocha test/main.cjs",
		"format": "prettier . --write",
		"// TODO cov script not currently collecting coverage information although it runs": "",
		"cov": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -R test/main.cjs",
		"doc": "jsdoc2md -f tmp/kiwi.js -m none -g none > docs/Kiwi.md && node scripts/docs-update-headings.js && prettier docs/Kiwi.md --write",
		"lint": "prettier . --check",
		"bench": "node bench/main.cjs"
	}
}
